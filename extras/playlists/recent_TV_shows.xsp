<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Recent TV shows</name>
    <match>all</match>
    <rule field="playcount" operator="lessthan">
        <value>1</value>
    </rule>
    <limit>20</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
