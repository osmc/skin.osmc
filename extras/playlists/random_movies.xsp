<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Random movies</name>
    <match>all</match>
	<rule field="playcount" operator="lessthan">
        <value>1</value>
	</rule>
    <limit>20</limit>
    <order direction="descending">random</order>
</smartplaylist>
