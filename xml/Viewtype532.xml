<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<include name="Viewtype532">

		<!-- Wall small -->
		<control type="group">
			<visible>Control.IsVisible(532)</visible>

			<!-- Thumbs -->
			<control type="panel" id="532">
				<include>Viewtype532_coords1</include>
				<onleft>63</onleft>
				<onup>532</onup>
				<ondown>532</ondown>
				<pagecontrol>63</pagecontrol>
				<preloaditems>4</preloaditems>
				<viewtype label="$LOCALIZE[31114]">icon</viewtype>
				<scrolltime tween="sine" easing="out">240</scrolltime>
				<visible>Container.Content(movies) | Container.Content(sets) | Container.Content(tvshows) | Container.Content(seasons)</visible>

				<include>Viewtype532_coords2</include>

				<include>Viewtype532_coords3</include>
				
			</control>

			<control type="group">
				<include>Viewtype532_coords4</include>

				<!-- Title -->
				<control type="fadelabel">
					<include>Viewtype532_coords5</include>
					<font>Font42</font>
					<align>center</align>
					<textcolor>$VAR[TextColorFO]</textcolor>
					<label>$VAR[Label1]</label>
					<scrollspeed>120</scrollspeed>
				</control>

				<!-- Details -->
				<control type="fadelabel">
					<include>Viewtype532_coords6</include>
					<font>Font27</font>
					<align>center</align>
					<aligny>top</aligny>
					<label>$VAR[Label2]</label>
					<textcolor>$VAR[TextColorFO]</textcolor>
				</control>

			</control>

		</control>

	</include>

</includes>
