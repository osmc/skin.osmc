<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font33</font>
		<textcolor>$VAR[TextColor1]</textcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<selectedcolor>$VAR[SelectedColor]</selectedcolor>
		<scroll>false</scroll>
	</default>

	<default type="fadelabel">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font33</font>
		<textcolor>$VAR[TextColor1]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>

	<default type="button">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font33</font>
		<label></label>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<invalidcolor>$VAR[InvalidColor]</invalidcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
		<texturenofocus></texturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="togglebutton">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<label></label>
		<textoffsetx>0</textoffsetx>
		<font>Font33</font>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturenofocus></texturenofocus>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
		<alttexturenofocus></alttexturenofocus>
		<alttexturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</alttexturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font33</font>
		<label></label>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<radiowidth>16</radiowidth>
		<radioheight>16</radioheight>
		<texturenofocus></texturenofocus>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
		<textureradioonfocus colordiffuse="$VAR[DialogColor1]">common/RadioOn.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[DialogColor1]">common/RadioOn.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="$VAR[DialogColor1]">common/RadioOff.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="$VAR[DialogColor1]">common/RadioOff.png</textureradiooffnofocus>
		<textoffsetx>0</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<label></label>
		<font>Font33</font>
		<spinwidth>35</spinwidth>
		<spinheight>66</spinheight>
		<textureup colordiffuse="$VAR[DialogColor2]">common/ArrowUpFO.png</textureup>
		<texturedown colordiffuse="$VAR[DialogColor2]">common/ArrowDownFO.png</texturedown>
		<textureupfocus colordiffuse="$VAR[DialogColor1]">common/ArrowUpFO.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[DialogColor1]">common/ArrowDownFO.png</texturedownfocus>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>0</textoffsetx>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
	</default>

	<default type="spincontrol">
		<height>66</height>
		<width>66</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup colordiffuse="$VAR[DialogColor2]">common/ArrowUpFO.png</textureup>
		<texturedown colordiffuse="$VAR[DialogColor2]">common/ArrowDownFO.png</texturedown>
		<textureupfocus colordiffuse="$VAR[DialogColor1]">common/ArrowUpFO.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[DialogColor1]">common/ArrowDownFO.png</texturedownfocus>
		<font>Font33</font>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>0</textoffsetx>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
	</default>

	<default type="sliderex">
		<height>66</height>
		<width>183</width>
		<align>left</align>
		<aligny>center</aligny>
		<texturesliderbar border="10,0,10,0">osd/OSDSliderBack.png</texturesliderbar>
		<textureslidernib border="10,0,10,0" colordiffuse="$VAR[TextColor2]">osd/OSDSliderNib.png</textureslidernib>
		<textureslidernibfocus border="10,0,10,0" colordiffuse="$VAR[TextColor1]">osd/OSDSliderNib.png</textureslidernibfocus>
		<sliderwidth>230</sliderwidth>
		<sliderheight>23</sliderheight>
		<font>Font33</font>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>5</textoffsetx>
		<orientation>horizontal</orientation>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
	</default>

	<default type="slider">
		<width>360</width>
		<height>23</height>
		<texturesliderbar border="10,0,10,0">osd/OSDSliderBack.png</texturesliderbar>
		<textureslidernib border="10,0,10,0" colordiffuse="$VAR[TextColor2]">osd/OSDSliderNib.png</textureslidernib>
		<textureslidernibfocus border="10,0,10,0" colordiffuse="$VAR[TextColor1]">osd/OSDSliderNib.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
	</default>

	<default type="progress">
		<height>44</height>
		<texturebg></texturebg>
		<lefttexture></lefttexture>
		<righttexture></righttexture>
		<overlaytexture></overlaytexture>
	</default>

	<default type="textbox">
		<align>justify</align>
		<font>Font27</font>
		<textcolor>$VAR[TextColor2]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">True</autoscroll>
	</default>

	<default type="edit">
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font33</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[TextColor2]</textcolor>
		<focusedcolor>$VAR[TextColor1]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<invalidcolor>$VAR[InvalidColor]</invalidcolor>
		<texturefocus colordiffuse="$VAR[buttonfocus]">common/focus.png</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="fixedlist">
		<preloaditems>2</preloaditems>
	</default>

</includes>